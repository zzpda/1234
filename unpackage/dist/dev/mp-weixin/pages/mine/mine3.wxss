page {
	height:100%;
	width: 100%;
	background: #f6f6f6;
	/*background: linear-gradient(#5595ff, #8739e6); /* 标准的语法 *\/
 */
}
.container{
	height: 100%;/*固定长度
 */
	width: auto;
	/* padding-left: 10rpx;
 */
	/* padding-right: 10rpx;
 */
	display: flex;
	flex-direction: column;
	align-items: center;
}
.top-box{
	width:100%;
	height:40%;
	display: flex;
	flex-direction: column;
	align-items: center;
	background-image:url(data:image/png;base64,);
}
.name{
	margin-top: 10rpx;
}
.img-top{
	width: 160rpx;
	height: 160rpx;
	margin-top: 80rpx;
}
.center-box{
	width: 90%;
	/* height: 220rpx;
 */
	position: relative;
	background: #7981b6;
	border: 15rpx;
	border-radius: 25rpx 25rpx 0 0;
	margin-top: -150rpx;
	display: flex;
	flex-direction: row;
	justify-content: space-around;
	align-items: center;
}
.bottom-box{	
		margin-top: 30rpx;
		width:90%;
		flex:1;
		background:#FFFFFF;	 
		display: flex;
		justify-content: space-between;
		flex-wrap: wrap;
		border-radius: 25rpx;
}
.centeritem-box{
	width: 200rpx;
	height: 150rpx;
	margin: 0 30rpx;
	/*background: #4CD964;
 */
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.img-item{
	width: 160rpx;
	height: 160rpx;
}
.item-box{
	width: 110rpx;
	margin: 0 50rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.itemnameTxt{
	font-size:26rpx ;
	line-height:40rpx;
}
.btn-box{
	height: 100rpx;
	width: 90%;
	background: #09b8f3;
	border-radius: 45rpx;
	margin: 30rpx 0rpx;
	font-size: 40rpx;
	line-height: 100rpx;
	text-align: center;
	color: #FFFFFF;
}

